#include<stdio.h>
int main()
{
    int a,b,c,d;

    printf("enter a,b,c,d");
    scanf("%d%d%d%d",&a,&b,&c,&d);
    

    if(a>b && a>c && a>d)
{
    printf("a is greatest among these number which is %d", a);
}


else if(b>a && b>c && b>d)
{
    printf("b is greatest among these number which is %d", b);
}



else if(c>a && c>b &&c>d)
{
    printf("c is greatest among these number which is %d", c);
}




else if(d>b && d>c && d>a)
{
    printf("d is greatest among these number which is %d", d);
}

return 0;

}
